//GCC_WARN_SHADOW = YES // 隐式转换成32位的类型
GCC_WARN_ABOUT_MISSING_FIELD_INITIALIZERS = YES
GCC_WARN_MULTIPLE_DEFINITION_TYPES_FOR_SELECTOR = YES //严格的Selector匹配
GCC_WARN_ABOUT_MISSING_FIELD_INITIALIZERS = YES
GCC_WARN_ABOUT_MISSING_PROTOTYPES = YES //缺 少函数原型
GCC_WARN_STRICT_SELECTOR_MATCH = YES //选择了多个定义的类型
GCC_TREAT_IMPLICIT_FUNCTION_DECLARATIONS_AS_ERRORS = YES //把缺少函数原型当作错误

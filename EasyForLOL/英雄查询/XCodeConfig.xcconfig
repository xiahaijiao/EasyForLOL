GCC_WARN_UNDECLARED_SELECTOR = YES  //未定义的Selector
GCC_WARN_UNINITIALIZED_AUTOS = YES //未初始化的自动变量
GCC_WARN_UNKNOWN_PRAGMAS = YES //未知的Pragma指令
GCC_WARN_UNUSED_FUNCTION = YES //未使用的函数
GCC_WARN_UNUSED_LABEL = YES //未使用的标签
GCC_WARN_UNUSED_VALUE = YES //未使用的值
GCC_WARN_UNUSED_VARIABLE = YES //当一个语句计算的结果显式的未使用的时候发出警告 未使用的变量
GCC_WARN_ABOUT_DEPRECATED_FUNCTIONS = YES //警告－所有过时的函数
GCC_WARN_ABOUT_INVALID_OFFSETOF_MACRO = YES //offsetof宏未定义使用的警告